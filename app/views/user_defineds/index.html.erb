<h1>Listing user_defineds</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_defineds.each do |user_defined| %>
      <tr>
        <td><%= link_to 'Show', user_defined %></td>
        <td><%= link_to 'Edit', edit_user_defined_path(user_defined) %></td>
        <td><%= link_to 'Destroy', user_defined, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User defined', new_user_defined_path %>
